pipeline {
    agent any
    stages {
        stage('Build') { 
            steps {
                bat 'mvn -B -DskipTests clean package' 
}
}
stage('Test') { 
            steps {
                bat 'mvn test'
            }
        }
stage('install') {
            steps {
                bat 'mvn install'
            }
        }
stage('Deliver') {
            steps {
                bat 'deliver'
            }
        }
    }
}

